{
    "name": "adam-paterson/ibm-watson-sdk",
    "type": "library",
    "description": "",
    "keywords": [
        "ibm",
        "watson",
        "machine learning"
    ],
    "homepage": "https://github.com/adam-paterson/ibm-watson-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Paterson",
            "email": "hello@adampaterson.co.uk",
            "homepage": "https://github.com/adam-paterson",
            "role": "Developer"
        },
        {
            "name": "All Contributors",
            "homepage": "https://github.com/adam-paterson/ibm-watson-sdk/contributors"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/http-foundation": "^3.2"
    },
    "replace": {
        "adam-paterson/watson-common": "self.version"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "IBM\\Watson\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --ignore=*/Tests/* --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --ignore=*/Tests/* --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
